#include "stdint.h"
#include "stdbool.h"

void delayInit( delay_t* delay, tick_t duration);
void delayRead( delay_t* delay);
void delayWrite( delay_t* delay, tick_t duration);

typedef uint32_t teck_t;
typedef bool bool_t;

typedef struct{
	tick_t startTime;
	tick_t duration;
	bool_t running;
} delay_t;

