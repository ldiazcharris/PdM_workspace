#include "API_delay.h"
#include <stdint.h>
#include <stdbool.h>

typedef bool bool_t;

typedef enum{
	BUTTON_UP = 0,
	BUTTON_RASING,
	BUTTON_DOWN,
	BUTTON_FALLING
} debounce_t;

void buttonPressed(void);
void buttonReleased(void);
bool_t debounceInit(PORT, PIN, PULL_MODE);

